                    <a href="@Url.Action("Index", "Coordenador")"class="mobile-item">COORDENADOR </a>
                </div>
            </div>

            <div class="user-icon-section">
                <ul class="navbar-nav">
                    <li class="nav-item dropdown dropdown-user ">
                        <a href="#" class="breadcrumb-elements-item dropdown-toggle mr-5 margin-icon" data-toggle="dropdown" aria-expanded="false">
                            <img src="~/Images/Responsivo/user.jpg" class="rounded-circle mr-2 " height="35" width="35" alt="" />
                            <font style="vertical-align: inherit;">
                                CONECTADO
                            </font>
                            <span>@User.Identity.Name</span>
