                <div class=" ml-4 font-size-xs opacity-50">
                    <i class="icon-pin font-size-sm"></i> &nbsp;Curitiba-PR
                </div>
            </div>

            <div class="menu-mobile-header">
                <a class="icon close-menu" href="#">
                    <i class="icon-paragraph-justify3"></i>
                </a>
                <div class="menu-links">
                    <a href="@Url.Action("Index","Professor")" class="mobile-item">PROFESSOR </a>
                    <a href="@Url.Action("Index","Modalidade")" class="mobile-item">MODALIDADE </a>
                    <a href="@Url.Action("Index","Curso")" class="mobile-item">CURSO </a>
                    <a href="@Url.Action("Index","UnidadeCurricular")"class="mobile-item">UNIDADE CURRICULAR </a>
